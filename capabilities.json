{
  "dataRoles": [
    {
      "name": "Line",
      "displayName": "Linhas",
      "displayNameKey": "Visual_Line",
      "kind": "Grouping"
    },
    {
      "name": "Column",
      "displayName": "Colunas",
      "displayNameKey": "Visual_Column",
      "kind": "Grouping"
    },
    {
      "name": "Value",
      "displayName": "Valores",
      "displayNameKey": "Visual_Values",
      "kind": "Measure"
    }
  ],
  "dataViewMappings": [
    {
      "matrix": {
        "rows": {
          "for": {
            "in": "Line"
          }
        },
        "columns": {
          "for": {
            "in": "Column"
          }
        },
        "values": {
          "select": [
            {
              "for": {
                "in": "Value"
              }
            }
          ]
        }
      }
    }
  ],
  "objects": {
    "dataPoint": {
      "properties": {
        "defaultColor": {
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "showAllDataPoints": {
          "type": {
            "bool": true
          }
        },
        "fill": {
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "fillRule": {
          "type": {
            "fill": {}
          }
        },
        "fontSize": {
          "type": {
            "formatting": {
              "fontSize": true
            }
          }
        }
      }
    }
  },
  "privileges": []
}
